$(".toOpen").click(function(){
    var hasOpen = $(this).hasClass("isOpen");
    if(hasOpen){
        $(this).next().hide();
        $(this).removeClass("isOpen");
    }else{
        $(this).next().show();
        $(this).addClass("isOpen");
    }
})
</script> 
